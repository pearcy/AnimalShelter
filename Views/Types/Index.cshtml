@{
  Layout = "_Layout";
}

@using AnimalShelter.Models;
@model List<AnimalShelter.Models.Type>;

<h1>Types</h1>

  <ul>
  @foreach(AnimalShelter.Models.Type type in Model)
  {
      <li>@Html.ActionLink($"{type.Description}", "Details", new { id = type.TypeId })</li>
  }
</ul>

<p>@Html.ActionLink("Add new item", "Create")</p>